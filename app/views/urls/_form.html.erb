<div class="container">
<% if url.errors.any? %>
  <% url.errors.full_messages.each do |message| %>
    <li><%= message %></li>
  <% end %>
<% end %>
<div class="form-group">
  <%= f.label :description %>
  <%= f.text_field :description, class:"form-control"%>
</div>
<div class="form-group">
  <%= f.label :long_url %>
  <%= f.text_field :long_url, class:"form-control" %>
</div>
<div class="form-group">
  <%= f.submit class: 'btn btn-danger' %>
</div>
</div>